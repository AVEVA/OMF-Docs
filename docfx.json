{  
  "build": {
    "globalMetadata": {
      "_enableSearch": true,
      "_appLogoPath": "./images/atlas_icon_small.png",
      "_appFooter": "<a href=\"https://www.osisoft.com/copyright/\"> Â© 2018 - 2020 OSIsoft, LLC. All rights reserved.</a>",
      "_appFaviconPath": "./images/favicon.png",
      "_gitContribute": {
       "branch": "main"
      }
    },
    "content": [
      {
        "files": [
          "documentation/**/**.md",
          "documentation/**/toc.yml",
          "toc.yml",
          "**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "_exported_templates/last-modified"
    ],
    "postProcessors": [
      "LastModifiedPostProcessor"
    ],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
